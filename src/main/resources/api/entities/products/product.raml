#%RAML 1.0 DataType
type: object
properties:
  Code:
    type: string
  Name:
    type: string
  Description:
    type: string
  ProductType:
    enum:
    - Checking
    - Savings
    - CD
    - Mortgage
    - CreditCard
    type: string
  InterestPlans?:
    type: array
    items: !include interestPlan.raml